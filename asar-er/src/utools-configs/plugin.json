{
  "pluginName": "asar-er",
  "author": "ChandlerVer5",
  "homepage": "https://www.kancloud.cn/@chandler",
  "description": "asar助手，查看asar、搜索🔍 utools插件等😄",
  "version": "0.0.1",
  "logo": "logo.png",
  "main": "index.html",
  "preload": "preload.js",
  "pluginSetting": {
    "single": true
  },
  "development": {
    "main": "http://localhost:8080"
  },
  "features": [
    {
      "code": "asar_er",
      "explain": "asar助手、分析器",
      "cmds": [
        "asar",
        "upx",
        {
          "type": "files",
          "label": "asar analyse",
          "fileType": "file",
          "match": "/.asar$/",
          "maxLength": 1
        }
      ]
    }
  ]
}
