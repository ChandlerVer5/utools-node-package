{
  "name": "FFFFFFFF",
  "version": "5.0.0",
  "pluginName": "管理中心",
  "main": "index.html",
  "logo": "logo.png",
  "preload": "preload.js",
  "features": [
    {
      "code": "store",
      "explain": "插件应用市场",
      "cmds": [
        "插件应用市场"
      ]
    },
    {
      "code": "installed",
      "explain": "已安装插件应用",
      "cmds": [
        "已安装应用"
      ]
    },
    {
      "code": "settings",
      "explain": "我的账号、设置、本地文件启动、全局快捷键、数据存储",
      "cmds": [
        "个人中心"
      ]
    },
    {
      "code": "featurecmds",
      "explain": "查看所有功能关键字",
      "cmds": [
        "所有关键字"
      ]
    },
    {
      "code": "connection",
      "explain": "局域网内同帐号，设备间资源互传",
      "cmds": [
        "检测我的设备间资源互传"
      ]
    },
    {
      "code": "plugininstall",
      "cmds": [
        {
          "type": "files",
          "fileType": "file",
          "match": "/\\.upx$/i",
          "label": "插件应用安装",
          "maxLength": 1
        }
      ]
    },
    {
      "code": "settings-localopen",
      "explain": "文件或文件夹加入「本地文件启动」",
      "cmds": [
        {
          "type": "files",
          "maxLength": 1,
          "label": "加入「本地文件启动」"
        }
      ]
    },
    {
      "code": "redeemcode",
      "cmds": [
        {
          "type": "regex",
          "match": "/^uTools-[0-9a-zA-Z]{32}-[A-Z]{2}$/",
          "label": "uTools 兑换码",
          "minLength": 42,
          "maxLength": 42
        }
      ]
    }
  ]
}